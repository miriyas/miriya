model Author {
  id String @id @default(cuid())
  MyCarParts     MyCarParts[]
}

model MyCar {
  id String @id @default(cuid())
  MyCarParts MyCarParts[]
}

model MyCarParts {
  id        String    @id @unique @default(cuid())
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
  author    Author    @relation(fields: [authorId], references: [id])
  authorId  String
  car       MyCar     @relation(fields: [carId], references: [id])
  carId     String
  name      String
  body      String
  partsNo   String
  partsUrl  String?
}